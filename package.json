{
  "name": "mx-react-components",
  "version": "5.0.0-rc.1",
  "description": "A collection of generic React UI components",
  "main": "dist/Index.js",
  "scripts": {
    "release": "eslint src && babel src --out-dir dist",
    "compile:docs": "cd docs rm -rf node_modules && npm install && NODE_ENV=production webpack --progress && git add -A && git commit -m $(node -p \"'Docs Version: ' + require('./package.json').version\")",
    "dev": "concurrently --kill-others 'npm run docs:server' 'npm run docs:webpack'",
    "docs:server": "npm install && open 'http://localhost:8080' && webpack-dev-server --content-base docs/",
    "docs:webpack": "cd docs && NODE_ENV=development webpack --watch",
    "test": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moneydesktop/mx-react-components"
  },
  "keywords": [
    "react",
    "components",
    "mx",
    "UI",
    "library"
  ],
  "author": "MX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moneydesktop/mx-react-components/issues"
  },
  "homepage": "http://moneydesktop.github.io/mx-react-components/",
  "dependencies": {
    "d3": "^3.5.6",
    "lodash": "^4.6.1",
    "moment": "^2.10.3",
    "moment-timezone": "^0.5.4",
    "numeral": "^1.5.3",
    "radium": "^0.18.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "velocity-animate": "^1.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "concurrently": "^2.2.0",
    "eslint": "^2.10.0",
    "eslint-config-mx": "^1.2.7",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^5.0.0",
    "live-server": "^0.8.1",
    "node-libs-browser": "^0.5.2",
    "webpack": "^1.0.0"
  }
}
